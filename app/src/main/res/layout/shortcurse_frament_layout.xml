<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/image_size_height"
    android:foreground="?attr/selectableItemBackground">

    <!--<View-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="1dp"-->
    <!--android:alpha=".5"-->
    <!--android:background="@android:color/white" />-->

    <RelativeLayout
        android:id="@+id/lecture_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!--<View-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="2dp"-->
        <!--android:alpha=".5"-->
        <!--android:background="@android:color/white" />-->
        <TextView
            android:id="@+id/tv_data"
            style="?android:textAppearanceSmallInverse"
            android:layout_width="@dimen/image_size_width"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:background="@color/text_background"
            android:gravity="center"
            android:padding="@dimen/tiny_margin"
            android:text="18 JUN"
            android:textSize="26sp"
            android:textStyle="normal"
            />


        <TextView
            android:id="@+id/tv_titulo"
            style="?android:textAppearanceMedium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/tiny_margin"
            android:layout_marginRight="16dp"
            android:layout_toEndOf="@+id/tv_data"
            android:ellipsize="end"
            android:maxLines="1"
            android:paddingLeft="@dimen/small_margin"
            android:paddingRight="@dimen/small_margin"
            tools:text="Internet da Coisas"
            android:textStyle="bold"

            />

        <TextView
            android:id="@+id/tv_palestrante"
            style="?android:textAppearanceSmall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_titulo"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="16dp"
            android:layout_toEndOf="@+id/tv_data"

            android:ellipsize="end"
            android:maxLines="3"
            android:padding="@dimen/small_margin"
            tools:text="Emerson Maranhao" />

        <TextView
            android:id="@+id/tv_local"
            style="?android:textAppearanceSmall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/tv_palestrante"
            android:layout_below="@+id/tv_palestrante"
            android:ellipsize="end"
            android:layout_marginRight="16dp"

            android:maxLines="3"
            android:padding="@dimen/small_margin"
            tools:text="Bloco B sala 5" />

        <TextView
            android:id="@+id/tv_horaInicio"
            style="?android:textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/view"
            android:layout_marginEnd="0dp"
            android:layout_toStartOf="@+id/textView"
            android:gravity="right"
            android:maxLines="1"
            android:padding="@dimen/small_margin"
            tools:ignore="RtlHardcoded"
            tools:text="18:30h" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/tv_horaFim"
            android:layout_alignBottom="@+id/tv_horaFim"
            android:layout_marginEnd="0dp"
            android:layout_toStartOf="@+id/tv_horaFim"
            android:text="Ã s"
            android:id="@+id/textView" />

        <TextView
            android:id="@+id/tv_horaFim"
            style="?android:textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/view"
            android:layout_alignEnd="@+id/tv_local"
            android:gravity="right"
            android:maxLines="1"
            android:padding="@dimen/small_margin"
            tools:ignore="RtlHardcoded"
            tools:text="20:30h" />

        <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="4dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:alpha=".5"
            android:background="@android:color/white" />


    </RelativeLayout>

</FrameLayout>


